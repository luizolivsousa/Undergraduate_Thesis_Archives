DEFVAL        TDEIO1   0.25       # TEMPO DE DEIONIZAÇÃO - 1ª TENTATIVA
DEFVAL        TDEIO2   0.35       # TEMPO DE DEIONIZAÇÃO - 2ª TENTATIVA
DEFVAL        TDEIO3   0.45       # TEMPO DE DEIONIZAÇÃO - 3ª TENTATIVA
DEFVAL        TDEIO4   0.55       # TEMPO DE DEIONIZAÇÃO - 4ª TENTATIVA
DEFVAL        TDEIO5   0.55       # TEMPO DE DEIONIZAÇÃO - IDEM A 4ª TENTATIVA
DEFVAL        TDEIO6   0.55       # TEMPO DE DEIONIZAÇÃO - IDEM A 4ª TENTATIVA
